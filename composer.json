{
    "name": "teggoin/gravity-editor-bundle",
    "description": "Symfony 8.1 bundle for gravity-ui/markdown-editor integration (forms, EasyAdmin 5, Twig)",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.4|^7.0|^8.0",
        "symfony/dependency-injection": "^6.4|^7.0|^8.0",
        "symfony/form": "^6.4|^7.0|^8.0",
        "symfony/framework-bundle": "^6.4|^7.0|^8.0",
        "symfony/twig-bundle": "^6.4|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.4|^7.0|^8.0"
    },
    "suggest": {
        "easycorp/easyadmin-bundle": "^5.0 - For EasyAdmin 5 Field integration",
        "twig/extra-bundle": "^3.0 - For markdown_to_html in EasyAdmin field template"
    },
    "autoload": {
        "psr-4": {
            "GravityUI\\MarkdownEditorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityUI\\MarkdownEditorBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
